<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE queryservice PUBLIC "-//ANYFRAME//DTD QUERYSERVICE//EN"
"http://www.anyframejava.org/dtd/anyframe-core-query-mapping-3.2.dtd">
<queryservice>
	<queries>
		<query id="findCustContractList" isDynamic="true">
			<statement>
				<!--foundation findMovieList-START -->
				SELECT rownum as no,
				cu.cnm || '(' || cu.cus_no || ')' as cus_name_no,
				co.code_name as incoStacC,
				cc.entr_date ,
				cc.retr_date
				from custom cu, enmn_cont cc, code co
				where cc.cus_no = cu.cus_no
				  and cc.inco_stac_c = co.code
			</statement>
		</query>

		<query id="findCustContractByPk">
			<statement>  계약번호 고객번호
			             재직상태코드
			             가입일자 퇴사일자
				SELECT cont_no,
				cus_no,
				co.code_name as incoStacC,
				cc.entr_date,
				cc.retr_date
				FROM enmn_cont cc, code co
				WHERE 1=1
				and cont_no = :vo.contNo
				and cus_no = :vo.cusNo
			</statement>
			<result class="com.sds.ps.domain.CustContract" />
		</query>


