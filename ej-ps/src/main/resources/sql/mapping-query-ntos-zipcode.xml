<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE queryservice PUBLIC "-//ANYFRAME//DTD QUERYSERVICE//EN"
"http://www.anyframejava.org/dtd/anyframe-core-query-mapping-3.2.dtd">
<queryservice>
	<queries>
		<query id="findZipcodeList">
			<statement>
				SELECT zip1, zip2, sido, gugun, dong, bunji
				FROM zipcode
				where dong like '%' || :vo.searchKeyword || '%'
			</statement>
			<result class="com.sds.ps.domain.Zipcode" />
		</query>
	</queries>
</queryservice>

